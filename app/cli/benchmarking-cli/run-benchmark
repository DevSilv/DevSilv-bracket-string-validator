#!/bin/bash

ARG="$1"

if [ -f "$ARG" ] ; then
    node "./app/logic/benchmarking/non-testable/run-benchmark.js" "$ARG"
elif [ "$ARG" == "" ] ; then
    node "./app/logic/benchmarking/non-testable/run-benchmark.js"
elif [ -e "$ARG" ] ; then
    printf "File of wrong type specified: $ARG. You may use only regular files.\n"
else
    printf "The specified file does not exist: $ARG.\n"
fi